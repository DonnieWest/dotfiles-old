Xft.dpi: 277
Xcursor.size: 50
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 0
Xft.rgba: rgb

URxvt.font: xft:FuraMono Nerd Font Mono:size=6:antialias=false
URxvt.boldFont: xft:FuraMono Nerd Font Mono:size=6
URxvt.letterSpace: -3
URxvt.scrollBar: off

XTerm*.LoginShell: true
XTerm*VT100.geometry: 80x24
XTerm.termName: xterm-256color
XTerm*VT100.internalBorder: 0
XTerm*borderWidth: 0
XTerm*dynamicColors: true
XTerm*utf8: 1
XTerm*utf8Fonts: 1
XTerm*saveLines: 2048
XTerm*scrollKey: true
XTerm*scrollTtyOutput: false
XTerm*scrollBar: false
XTerm*toolBar: false
XTerm*metaSendsEscape: true
XTerm*altIsNotMeta: true
XTerm*altSendsEscape: true
XTerm*decTerminalID: vt340
XTerm*numColorRegisters: 256
xterm*eightBitInput: false
XTerm*faceName: FuraMono Nerd Font Mono:style=Regular:antialias=false
XTerm*faceSize: 6
XTerm*scaleHeight: 0.95
xterm*saveLines: 16384
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*VT100.Translations: #override \
    Shift <KeyPress> Insert: insert-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>C:    copy-selection(CLIPBOARD) \n\
    Meta <Btn1Up>: exec-formatted("xdg-open '%t'", PRIMARY)
!! Only select text
XTerm*highlightSelection: true
XTerm*selectToClipboard:true
XTerm*fullscreen: never

